{
  "rewrites": [
    { 
      "source": "/(.*)\\.(json|js|css|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot)",
      "destination": "/$1.$2"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)\\.(.*)$",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
